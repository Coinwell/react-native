{
  "name": "ZION",
  "description": "Bitcoin Lightning Powered Chat",
  "author": "",
  "version": "1.13.0",
  "main": "public/electron",
  "homepage": "./",
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
    "android-backup": "BABEL_ENV=dev react-native run-android && adb logcat *:S ReactNative:V ReactNativeJS:V",
    "ios": "react-native run-ios",
    "web": "react-app-rewired start",
    "build": "react-app-rewired build",
    "jetify": "jetify",
    "xcode": "open ios/Chat.xcworkspace",
    "bump": "npx react-native bump-version --skip-semver-for android --skip-code-for android",
    "t": "bash -c 'yarn tsc --noEmit -p ./tsconfig.json'",
    "prepare": "husky install",
    "lint": "eslint --fix",
    "prettier": "prettier {src,web} --write",
    "electron": "ELECTRON_DEV_URL=http://localhost:3000 electron .",
    "install-app-deps": "electron-builder install-app-deps"
  },
  "lint-staged": {
    "{src,web}/**/*.{js,ts,jsx,tsx}": [
      "yarn lint",
      "yarn t"
    ]
  },
  "build": {
    "appId": "Zion.chat",
    "productName": "Zion Chat",
    "copyright": "copyright Â© 2021 ZION",
    "win": {
      "target": "nsis",
      "publisherName": "Zion",
      "icon": "build/static/icon.ico"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true
    },
    "mac": {
      "identity": null,
      "category": "chat.Zion.Chat",
      "icon": "build/static/icon.ico"
    },
    "linux": {
      "target": [
        "AppImage"
      ],
      "icon": "build/static/icon.png",
      "category": "Finance",
      "desktop": {
        "Name": "Zion",
        "Comment": "Lightning Chat",
        "Categories": "Network;Finance"
      }
    },
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "build"
    }
  },
